{
  "name": "filelinker",
  "version": "1.0.1",
  "description": "keeps hard links in sync.",
  "main": "filelinker.py",
  "documentationWebsite": {
    "trackingCode": "UA-40192634-18"
  },
  "scripts": {
    "document": "node_modules/boostnode/runnable/generateAPIDocumentation.py",
    "update": "npm run clear && npm version patch && npm publish",
    "updateDocumentation": "documentationWebsite --log-level info",
    "clear": "rm apiDocumentation --recursive --force && find './' -type f -name '*.pyc' -delete && find './' -type f -name '*.pyo' -delete"
  },
  "dependencies": {
    "boostnode": "latest"
  },
  "devDependencies": {
    "documentation-website": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/filelinker"
  },
  "keywords": [
    "python",
    "hardlink",
    "ensure",
    "service"
  ],
  "homepage": "http://torben.website/filelinker",
  "license": "CC-BY-3.0"
}
